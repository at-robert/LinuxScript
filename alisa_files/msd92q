    svn revert  tv-ap/dvb/ui2/res1920x1080x565/include/ZUI_SCREEN_SAVER_tables_h.inl
    svn revert  tv-ap/dvb/ui2/res1920x1080x565/include/ZUI_SUBTITLE_LANGUAGE_tables_h.inl
    svn revert  tv-ap/dvb/ui2/res1920x1080x565/include/ZUI_TENKEY_NUMBER_tables_h.inl
    svn revert  tv-ap/dvb/ui2/res1920x1080x565/include/ZUI_UPGRADE_tables_h.inl
    svn revert  tv-ap/dvb/ui2/res1920x1080x565/include/ZUI_components_h.inl
    svn revert  tv-ap/dvb/ui2/res1920x1080x565/include/ZUI_wndproc_h.inl
    else
    printf "$RED MSD92Q Invalid parameter $END \n"
    fi    
}

function merge_code_msd92q_setup()
{
    find . -type f -name "._.DS_Store" -exec rm {} \;
    find . -type f -name ".DS_Store" -exec rm {} \;
    svn del `svn status | sed -e '/^!/!d' -e 's/^!//'`
    svn status | grep '^\!' | cut -c8- | while read f; do svn rm "$f"; done
    svn add `svn status . | awk '/^[?]/{print $2}'`
    svn add . --force --no-ignore
}

function bu_msd92q_pwd()
{
    if [ "$1" == "sdk" ];
    then
    printf "$RED MSD92Q Copy Bmp bin $END \n"
    cp -a $MSTAR_BMP_MSD92Q_PATH/ZUI_bitmap.bin $MSTAR_BMP_MSD92Q_PATH/ZUI_bitmap_SW_LOAD.bin

    printf "$GREEN MSD92Q SDK Code $END \n"
    make clean; make all | tee $MAC_MSD92Q_PATH/debug.log
    elif [ "$1" == "amt" ];
    then
    printf "$RED MSD92Q AMT Code $END \n"
    ./make.sh cleanall | tee $MAC_MSD92Q_PATH/debug.log
    else
    echo "To copy MSD92Q BMP bin file into SW LOAD one"
    cp -a  $BMP_BIN_FILE $BMP_BIN_SW_FILE

    printf "$GREEN MSD92Q SDK Code $END \n"
    make clean; make all | tee $MAC_MSD92Q_PATH/debug.log    
    fi
}

function cp_msd92q_to(){
    VER=$1
    VM_IN=$MSTAR_MERGEBIN_MSD92Q_PATH
    VM_OUT=/media/psf

    if [ "$1" == "" ];
    then
        echo "Parameter shouldn't be empty"
        return
    fi

    if [ "$1" == "SPI_EMU" ];
    then
        VM_OUT=$MAC_MSD92Q_PATH
        echo " The output path will be SPI emulator $VM_OUT "
    fi

    if [ "$2" == "sdk" ];
    then
        echo "Copy Path : $MSTAR_MERGEBIN_MSD92Q_PATH"
        VM_IN=$MSTAR_MERGEBIN_MSD92Q_PATH
    elif [ "$2" == "sdk2" ];
    then
        echo "Copy Path : $MSTAR2_MERGEBIN_MSD92Q_PATH"
        VM_IN=$MSTAR2_MERGEBIN_MSD92Q_PATH
    elif [ "$2" == "sdk_ssd" ];
    then
        echo "Copy Path : $MSTARSSD_MERGEBIN_MSD92Q_PATH"
        VM_IN=$MSTARSSD_MERGEBIN_MSD92Q_PATH
    elif [ "$2" == "amt" ];
    then
        echo "Copy Path : $AMT_MERGEBIN_MSD92Q_PATH"
        VM_IN=$AMT_MERGEBIN_MSD92Q_PATH
    else
        echo "Copy Path : $MSTAR_MERGEBIN_MSD92Q_PATH"
        VM_IN=$MSTAR_MERGEBIN_MSD92Q_PATH
    fi

    echo "Liunx VM ENV"
    INPATH=$VM_IN/MERGE.bin
    OUTPATH=$VM_OUT/$VER/MERGE.bin

    if [ -d $VM_OUT/$VER ];
    then 
        printf "$GREEN "
    else
        printf "$RED $VM_OUT/$VER doesn't exist $END \n"
    return 
    fi

    if [ -f $INPATH ];
    then
        printf "$GREEN "
        ls -la $INPATH
    else
        printf "$RED $INPATH doesn't exist\n Don't forget to build the source code $END \n"
        return
    fi

    if [ -f $OUTPATH ];
    then
            echo "================================"
            echo "$OUTPATH exists so that deleting it ........"
            echo "================================"
            rm -rf $OUTPATH
    else
        echo "================================"
    fi

    echo "===Copy MSD92Q merge bin file to root folder =============="
    cp $INPATH $OUTPATH

    echo "===Copy MSD92Q merge bin file to smb folder =============="
    if [ -d $SMB_PATH ];
    then
    cp $INPATH $SMB_PATH/MERGE.bin
    else
        printf "$RED $SMB_PATH doesn't exist $END \n"
    fi
    printf "$END"
}